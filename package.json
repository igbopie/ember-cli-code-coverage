    "ember-cli-release": "^1.0.0-beta.1",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-exam": "1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-resolver": "^4.5.6",
    "ember-source": "~3.2.0",
    "eslint": "^4.19.1",
    "glob": "^7.1.2",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0"
  },
  "keywords": [
    "ember-addon"
  ],
