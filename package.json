    "fs-extra": "^6.0.1",
    "istanbul": "^0.4.3",
    "node-dir": "^0.1.16",
    "rsvp": "^4.8.2",
    "source-map": "^0.7.2",
    "string.prototype.startswith": "^0.2.0"
