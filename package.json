{
  "name": "ember-cli-nacho-coverage",
  "version": "1.0.2",
  "description": "Code coverage for ember projects using Istanbul regarless what transpiler you use.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "node-test": "mocha test/**/*-test.js",
    "browser-test": "COVERAGE=true ember test",
